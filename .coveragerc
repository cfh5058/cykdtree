[run]
plugins = Cython.Coverage
parallel = True
omit =
    */python?.?/*
    */lib-python/?.?/*.py
    */lib_pypy/_*.py
    */site-packages/ordereddict.py
    */site-packages/nose/*
    */unittest2/*
    *.pxd

[report]
sort = Cover
exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover
